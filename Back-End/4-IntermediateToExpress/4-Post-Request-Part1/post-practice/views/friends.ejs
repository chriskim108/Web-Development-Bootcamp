<h1>Friends List Goes Here!</h1>

<% friends.forEach(function(friend_value){ %>
    <li> <%= friend_value %> </li>
<% }); %>

<!--The action needs to match the URL link-->
<form action="/addfriend" method="POST">
    <!--When inputing a new name it will be stored inside "newfriend-->
    <input type="text" name="newfriend" placeholder="Name">
    <button>I made a new friend</button>
</form>

